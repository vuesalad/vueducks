{
  "name": "sworks-word-csv",
  "version": "1.0.0",
  "description": "word list by csv, basic infra   nodejs + src: all ts",
  "scripts": {
    "start": "npx tsc  && node dist/main.js",
    "util": "npx tsc  && node dist/filelist.js",
    "filecontent": "npx tsc && node dist/filecontent.js",
    "test:unit": "jest --coverage",
    "test:onlychanged": "jest --onlyChanged --coverage",
    "lint": "standard",
    "test": "npm run lint && npm run test:unit"
  },
  "keywords": [
    "cli"
  ],
  "author": "Saladworks",
  "license": "MIT",
  "dependencies": {
    "csv": "^6.0.5",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.0.5",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.21",
    "jest": "^25.2.6",
    "standard": "^14.3.3",
    "typescript": "^4.6.2"
  },
  "standard": {
    "globals": [
      "describe",
      "test",
      "expect",
      "jest"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
